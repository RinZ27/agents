{
  "$schema": "../../node_modules/wrangler/config-schema.json",
  "name": "context-management-demo",
  "main": "src/server.ts",
  "compatibility_date": "2026-01-28",
  "ai": {
    "binding": "AI",
    "remote": true
  },
  "compatibility_flags": ["nodejs_compat"],
  "assets": {
    "not_found_handling": "single-page-application",
    "run_worker_first": ["/agents/*"]
  },
  "durable_objects": {
    "bindings": [
      {
        "name": "ContextDemoAgent",
        "class_name": "ContextDemoAgent"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["ContextDemoAgent"]
    }
  ]
}
